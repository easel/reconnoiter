.SUFFIXES: .lo .@MODULEEXT@ .xml .xmlh

CC=@CC@
LD=@LD@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
PGCFLAGS=@PGCFLAGS@
MYCFLAGS=@MYCFLAGS@
SHCFLAGS=@SHCFLAGS@
MODULELD=@MODULELD@
MODULEEXT=@MODULEEXT@
LDFLAGS=@LDFLAGS@
AR=@AR@
RANLIB=@RANLIB@
LIBS=@LIBS@
PGLIBS=@PGLIBS@
MYLIBS=@MYLIBS@
INSTALL=@INSTALL@
MODULES_DIR=@MODULES_DIR@
XSLTPROC=@XSLTPROC@
XMLLINT=@XMLLINT@
XML2H=@top_srcdir@/buildtools/xml2h

top_srcdir=@top_srcdir@

OBJS=stomp.o

all:	libstomp.a

.c.o:
	@$(CC) $(CPPFLAGS) $(CFLAGS) @APRCFLAGS@ -I../libopenwire -c $<
	@echo "- compiling $<"

libstomp.a:	$(OBJS)
	@rm -f $@
	@$(AR) cq $@ $(OBJS)
	@echo "- archiving $@"
	@$(RANLIB) $@

clean:
	rm -f *.o *.a
