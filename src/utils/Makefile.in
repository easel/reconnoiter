CC=@CC@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
LDFLAGS=@LDFLAGS@
AR=@AR@
RANLIB=@RANLIB@
LIBS=@LIBS@
INSTALL=@INSTALL@

top_srcdir=@top_srcdir@

OBJS=noit_hash.o noit_skiplist.o noit_log.o noit_sem.o

all:	libnoit_utils.a

libnoit_utils.a:	$(OBJS)
	@rm -f $@
	@$(AR) cq $@ $(OBJS)
	@echo "- archiving $@"
	@$(RANLIB) $@

.c.o:
	@$(CC) $(CPPFLAGS) $(CFLAGS) -c $<
	@echo "- compiling $<"

clean:
	rm -f *.o libnoit_utils.a
